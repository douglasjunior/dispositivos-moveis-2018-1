<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <CheckBoxPreference
        android:defaultValue="@bool/pref_exibir_url_padrao"
        android:key="@string/pref_exibir_url"
        android:summaryOff="@string/nao_exibir"
        android:summaryOn="@string/exibir"
        android:title="@string/pref_exibir_url_titulo" />

    <ListPreference
        android:defaultValue="@string/pref_cor_fundo_padrao"
        android:entries="@array/pref_cor_fundo_lista_titulos"
        android:entryValues="@array/pref_cor_fundo_lista_valores"
        android:key="@string/pref_cor_fundo"
        android:title="@string/pref_cor_fundo_titulo"
        />

</PreferenceScreen>